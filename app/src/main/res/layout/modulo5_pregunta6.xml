<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/mod5_p6_lyt"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/fab_margin"
    android:padding="@dimen/margin15dp"
    android:background="@drawable/borde_layout_preguntas"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mod5_p6"
        android:layout_marginBottom="@dimen/fab_margin"/>
    <RadioGroup
        android:id="@+id/mod5_p6_rgb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin10dp">
        <RadioButton
            android:id="@+id/mod5_p6_rgb1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/si"/>
        <RadioButton
            android:id="@+id/mod5_p6_rgb2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/no"/>
        <RadioButton
            android:id="@+id/mod5_p6_rgb3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/no_sabe"/>
    </RadioGroup>
</LinearLayout>