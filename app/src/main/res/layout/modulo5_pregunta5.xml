<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/mod5_p5_lyt"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/fab_margin"
    android:padding="@dimen/margin15dp"
    android:background="@drawable/borde_layout_preguntas"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mod5_p5"
        android:layout_marginBottom="@dimen/fab_margin"/>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <CheckBox
            android:id="@+id/mod5_p5_ck1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/mod5_p5_sp1"
            android:layout_margin="@dimen/margin5dp"/>
        <CheckBox
            android:id="@+id/mod5_p5_ck2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/mod5_p5_sp2"
            android:layout_margin="@dimen/margin5dp"/>
        <CheckBox
            android:id="@+id/mod5_p5_ck3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/mod5_p5_sp3"
            android:layout_margin="@dimen/margin5dp"/>
        <CheckBox
            android:id="@+id/mod5_p5_ck4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/mod5_p5_sp4"
            android:layout_margin="@dimen/margin5dp"/>
        <CheckBox
            android:id="@+id/mod5_p5_ck5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/mod5_p5_sp5"
            android:layout_margin="@dimen/margin5dp"/>
        <CheckBox
            android:id="@+id/mod5_p5_ck6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/mod5_p5_sp6"
            android:layout_margin="@dimen/margin5dp"/>
    </LinearLayout>
</LinearLayout>