<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical"
    android:id="@+id/mod2_p9_lyt"
    android:focusableInTouchMode="true"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/fab_margin"
    android:padding="@dimen/margin15dp"
    android:background="@drawable/borde_layout_preguntas"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:id="@+id/mod2_p9_txtPregunta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mod2_p9"
        android:layout_marginBottom="@dimen/fab_margin"/>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin5dp">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/margin10dp"
            android:layout_marginRight="@dimen/margin10dp"
            android:layout_marginBottom="1dp">
            <TextView
                android:layout_weight="1"
                android:layout_marginRight="1dp"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="TIPO DE CONTRATO"
                android:textColor="@color/icons"
                android:textStyle="bold"
                android:gravity="center"
                android:padding="@dimen/margin10dp"
                android:background="@color/accent"/>
            <TextView
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:text="NÃšMERO"
                android:textColor="@color/icons"
                android:textStyle="bold"
                android:gravity="center"
                android:padding="@dimen/margin10dp"
                android:background="@color/accent"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/margin10dp"
            android:layout_marginRight="@dimen/margin10dp"
            android:layout_marginBottom="1dp">
            <CheckBox
                android:id="@+id/mod2_p9_ck1"
                android:layout_gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="@string/mod2_p9_sp1"
                android:padding="@dimen/margin10dp"
                android:layout_marginRight="1dp"
                android:background="@drawable/cajas_de_texto"/>
            <EditText
                android:id="@+id/mod2_p9_edt1"
                android:paddingLeft="@dimen/margin5dp"
                android:inputType="numberPassword"
                android:layout_width="100dp"
                android:enabled="false"
                android:maxLength="5"
                android:layout_height="match_parent"
                android:background="@drawable/cajas_de_texto_disabled"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/margin10dp"
            android:layout_marginRight="@dimen/margin10dp"
            android:layout_marginBottom="1dp">
            <CheckBox
                android:id="@+id/mod2_p9_ck2"
                android:layout_gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="@string/mod2_p9_sp2"
                android:layout_marginRight="1dp"
                android:padding="@dimen/margin10dp"
                android:background="@drawable/cajas_de_texto"/>
            <EditText
                android:id="@+id/mod2_p9_edt2"
                android:inputType="numberPassword"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/margin5dp"
                android:enabled="false"
                android:maxLength="5"
                android:background="@drawable/cajas_de_texto_disabled"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/margin10dp"
            android:layout_marginRight="@dimen/margin10dp"
            android:layout_marginBottom="1dp">
            <CheckBox
                android:id="@+id/mod2_p9_ck3"
                android:layout_gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="@string/mod2_p9_sp3"
                android:padding="@dimen/margin10dp"
                android:layout_marginRight="1dp"
                android:background="@drawable/cajas_de_texto"/>
            <EditText
                android:id="@+id/mod2_p9_edt3"
                android:inputType="numberPassword"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/margin5dp"
                android:enabled="false"
                android:maxLength="5"
                android:background="@drawable/cajas_de_texto_disabled"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/margin10dp"
            android:layout_marginRight="@dimen/margin10dp"
            android:layout_marginBottom="1dp">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_marginRight="1dp"
                android:background="@drawable/cajas_de_texto"
                android:layout_height="match_parent">
                <CheckBox
                    android:id="@+id/mod2_p9_ck4"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/mod2_p9_sp4"
                    android:padding="@dimen/margin10dp"
                    />
                <EditText
                    android:id="@+id/mod2_p9_edtEspecifique"
                    android:paddingLeft="@dimen/margin5dp"
                    android:layout_margin="5dp"
                    android:inputType="text"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:hint="@string/especifique"
                    android:maxLength="200"
                    android:enabled="false"
                    android:nextFocusForward="@+id/mod2_p9_edt4"
                    android:imeOptions="actionNext"
                    android:background="@drawable/fondo_edit_text_disabled"/>
            </LinearLayout>
            <EditText
                android:id="@+id/mod2_p9_edt4"
                android:inputType="numberPassword"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/margin5dp"
                android:enabled="false"
                android:maxLength="5"
                android:background="@drawable/cajas_de_texto_disabled"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/margin10dp"
            android:layout_marginRight="@dimen/margin10dp"
            android:layout_marginBottom="1dp">
            <CheckBox
                android:id="@+id/mod2_p9_ck5"
                android:layout_gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="@string/mod2_p9_sp5"
                android:padding="@dimen/margin10dp"
                android:layout_marginRight="1dp"
                android:background="@drawable/cajas_de_texto"/>
            <EditText
                android:id="@+id/mod2_p9_edt5"
                android:inputType="numberPassword"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/margin5dp"
                android:enabled="false"
                android:maxLength="5"
                android:background="@drawable/cajas_de_texto_disabled"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/margin10dp"
            android:layout_marginRight="@dimen/margin10dp"
            android:layout_marginBottom="@dimen/margin5dp">
            <TextView
                android:gravity="center"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="TOTAL"
                android:textColor="@color/icons"
                android:textStyle="bold"
                android:padding="@dimen/margin10dp"
                android:layout_marginRight="1dp"
                android:background="@color/colorAccent"/>
            <TextView
                android:id="@+id/mod2_p9_txtTotal"
                android:layout_width="100dp"
                android:paddingLeft="@dimen/margin5dp"
                android:layout_height="match_parent"
                android:text="0"
                android:textSize="20sp"
                android:gravity="center_vertical"
                android:background="@drawable/fondo_edit_text"/>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>